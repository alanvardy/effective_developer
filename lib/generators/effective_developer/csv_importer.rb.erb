module CsvImporters
  class <%= klass %>Importer < Effective::CSVImporter
    def columns
      {<% columns.each_with_index do |column, index| %>
        <%= column %>: <%= (letters[index] || index) %><%= ',' unless (index+1) == columns.length %><% end %>
      }
    end

    def process_row
      raise 'todo'
    end
  end
end
