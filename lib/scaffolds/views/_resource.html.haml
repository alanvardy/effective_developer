%table.table
  %tbody
<% resource.belong_tos.each do |reference| -%>
    %tr
      %th <%= reference.name.to_s.titleize %>
      %td= <%= resource.name %>.<%= reference.name %>
<% end %>
<% attributes.each do |attribute| -%>
    %tr
      %th <%= attribute.name.titleize %>
      %td= <%= resource.name %>.<%= attribute.name %>
<% end %>
